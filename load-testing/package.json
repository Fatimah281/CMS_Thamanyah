{
  "name": "cms-thamanyah-load-testing",
  "version": "1.0.0",
  "description": "Read-only load testing suite for CMS Thamanyah backend services - No database writes, only GET endpoint testing",
  "main": "k6-load-test-readonly.js",
  "scripts": {
    "test": "k6 run k6-load-test-readonly.js",
    "test:quick": "k6 run --stage 1m:10 --stage 2m:10 k6-load-test-readonly.js",
    "test:stress": "k6 run --stage 1m:50 --stage 5m:200 --stage 10m:500 k6-load-test-readonly.js",
    "test:full": "k6 run k6-load-test-readonly.js",
    "dashboard": "node performance-dashboard.js",
    "test:with-dashboard": "concurrently \"npm run dashboard\" \"npm run test\"",
    "test:quick:with-dashboard": "concurrently \"npm run dashboard\" \"npm run test:quick\"",
    "clean": "rm -rf *.json *.html",
    "help": "echo 'Available commands: npm run test, npm run test:quick, npm run test:stress, npm run test:full, npm run dashboard, npm run test:with-dashboard'"
  },
  "keywords": [
    "load-testing",
    "k6",
    "performance",
    "read-only",
    "api-testing",
    "get-endpoints"
  ],
  "author": "Thamanyah Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Fatimah281/cms-thamanyah.git"
  },
  "bugs": {
    "url": "https://github.com/Fatimah281/cms-thamanyah/issues"
  },
  "homepage": "https://github.com/Fatimah281/cms-thamanyah#readme"
}
